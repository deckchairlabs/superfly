{
  "name": "@deckchairlabs/superfly-monorepo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "preconstruct dev && manypkg check",
    "build": "preconstruct build",
    "postinstall": "preconstruct dev && manypkg check",
    "version:bump": "yarn changeset add && changeset version --snapshot",
    "prerelease": "yarn version:bump",
    "release": "yarn build && yarn changeset publish --access public --tag experimental"
  },
  "workspaces": [
    "packages/*"
  ],
  "preconstruct": {
    "packages": [
      "packages/*",
      "!packages/docs"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@changesets/cli": "^2.16.0",
    "@manypkg/cli": "^0.18.0",
    "@preconstruct/cli": "^2.1.0",
    "@types/node": "^15.12.1",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "typescript": "4"
  }
}
